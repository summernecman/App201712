<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:style_title="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
            name="frag_mana_store_list"
            type="String" />
    </data>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        android:orientation="vertical">
        <com.siweisoft.heavycenter.module.view.title.TitleView
            android:id="@+id/title"
            style_title:txt_mid ="仓库列表"
            style_title:iv_right ="@drawable/icon_scan"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </LinearLayout>
</layout>
